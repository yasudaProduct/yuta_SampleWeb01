{
  "MerinoSettings": {
    "SettingFile": {
      "Database": "database.json"
    },
    "InjectionAssembly": [
      {
        "AssemblyName": "SampleWeb01",
        "EndMatchNames": [
          {
            "EndMatchName": "Service"
          }
        ]
      },
      {
        "AssemblyName": "SampleWeb01.Infrastructure",
        "EndMatchNames": [
          {
            "EndMatchName": "Repository"
          }
        ]
      }
    ],
    "CustomSettingFile": [
      {
        "AssemblyName": "SampleWeb01",
        "ClassName": "SampleWeb01.Util.WebMvcSampleSetting",
        "FileName": "MerinoWebAppSettings.json"
      }
    ],
    "Env": 1,
    "EnvSetting": [
      {
        "EnvCls": 1,
        "EnvName": ""
      },
      {
        "EnvCls": 2,
        "EnvName": "develop"
      },
      {
        "EnvCls": 3,
        "EnvName": "st"
      },
      {
        "EnvCls": 4,
        "EnvName": "prod"
      }
    ],
    "Web": {
      "CustomErrorPage": "/Home/Error",
      "RootUrlPattern": "{controller=Home}/{action=Index}/{id?}"
    }
  }
}